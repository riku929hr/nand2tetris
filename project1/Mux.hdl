/** 
 * Multiplexor:
 * if (sel = 0) out = a, else out = b
 */
CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Not(in=sel, out=notSel);
    And(a=a, b=notSel, out=sel0);
    And(a=b, b=sel, out=sel1);
    Or(a=sel0, b=sel1, out=out);
}
